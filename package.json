{
  "name": "spindraad",
  "version": "1.9.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "run-p build:*",
    "build:vite": "remix vite:build",
    "storybook:build": "storybook build",
    "dev": "run-p dev:*",
    "dev:vite": "remix vite:dev",
    "dev:storybook": "storybook dev -p 6006 --no-open",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "sui": "shadcn-ui",
    "sui:add": "shadcn-ui add -y",
    "sui:diff": "shadcn-ui diff",
    "generate": "templates/plop-templates component --",
    "test": "vitest",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "@awesome.me/kit-feaffe80b3": "^1.0.7",
    "@fontsource-variable/chivo": "^5.0.14",
    "@fontsource-variable/manrope": "^5.0.20",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@remix-run/node": "^2.10.2",
    "@remix-run/react": "^2.10.2",
    "@remix-run/serve": "^2.10.2",
    "@sendgrid/mail": "^8.1.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "isbot": "^4.1.0",
    "lucide-react": "^0.378.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.5.0",
    "@remix-run/dev": "^2.10.2",
    "@remix-run/testing": "^2.10.2",
    "@storybook/addon-essentials": "^8.1.6",
    "@storybook/addon-interactions": "^8.1.6",
    "@storybook/addon-links": "^8.1.6",
    "@storybook/blocks": "^8.1.6",
    "@storybook/react": "^8.1.6",
    "@storybook/react-vite": "^8.1.6",
    "@storybook/test": "^8.1.6",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "plop": "^4.0.1",
    "postcss": "^8.4.38",
    "shadcn-ui": "^0.4.1",
    "storybook": "^8.1.6",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^1.6.0"
